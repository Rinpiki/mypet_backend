{
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": ["./src/@types", "./node_modules/@types"],
    "types": ["express"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": false
  },
  "include": ["src/**/*.ts", "src/@types/**/*.d.ts"]
}
